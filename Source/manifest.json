{
	"manifest_version": 2,

	"name": "Olympic Gold Medalist Henry Cejudo",
    "short_name": "Henry Cejudo",
	"description": "Reminds you that Henry Cejudo has a gold medal",
	"version": "0.1",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}